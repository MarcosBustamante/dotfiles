alias ga='git add'
alias gall='git add .'
alias gb='git branch'
alias gbl='git blame'
alias gc='git checkout'
alias gcl='git clone'
alias gcm='git commit -m'
alias gcma='git commit -am'
alias gd='git diff'
alias gf='git fetch'
alias gfr='git fetch && git rebase'
alias gl='git log'
alias glo='git log --oneline'
alias gpl='git pull'
alias gps='git push'
alias gra='git remote add'
alias grm='git rm'
alias grr='git remote rm'
alias gst='git status'
alias gpls='git pull origin master && git push origin $(gitbranch)'
alias git-remove-branchs='git branch -D $(git branch | grep -v '*\|master')'
